extends layout

block content
	#universe
		.wrapper
			.planet-wrapper
				.planet-img(style="background-image: url(../img/#{title}.jpg)")
			.info-wrapper

	ul.menu
		each item in items
			li
				a(href="/#{item.url}") #{item.name}


	script.
		var socket = io.connect();
		socket.on('urlchange', function (data) {
			window.location.href = data.url;
		});